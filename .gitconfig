[user]
	name = Messay Bekele
	email = mabekele@gmail.com
[color]
	ui = true
[alias]
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	hist-work = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short --max-count=10
	hist-work-long = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short --max-count=20
	log-graph = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''%C(white)%s%C(reset) %C(dim white)- %an%C(reset)' --all
